<p id="notice"><%= notice %></p>

<h1>Contractor Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contractor_types.each do |contractor_type| %>
      <tr>
        <td><%= contractor_type.name %></td>
        <td><%= link_to 'Show', contractor_type %></td>
        <td><%= link_to 'Edit', edit_contractor_type_path(contractor_type) %></td>
        <td><%= link_to 'Destroy', contractor_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contractor Type', new_contractor_type_path %>
