<p id="notice"><%= notice %></p>

<h1>Contractor Types</h1>

<table>
  <thead class="thead-dark">
    <tr class="text-center">
     <th scope="col" class="types">Types</th>
      <th colspan="3" class="actions">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @contractor_types.each do |contractor_type| %>
      <tr class="text-center">
      <td><%= contractor_type.name %></td>
        <td><%= link_to 'Show', contractor_type %></td>
        <td><%= link_to 'Edit', edit_contractor_type_path(contractor_type) %></td>
        <td><%= link_to 'Destroy', contractor_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<%= link_to '<button type="button" class="btn btn-dark">New contractor types</button>'.html_safe, new_contractor_type_path %>
<%= link_to '<button type="button" class="btn btn-dark">Back</button>'.html_safe, contractors_path %>